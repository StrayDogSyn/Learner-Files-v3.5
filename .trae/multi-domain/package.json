  "bundlesize": [
    {
      "path": "./dist/**/*.js",
      "maxSize": "100kb"
    },
    {
      "path": "./dist/**/*.css",
      "maxSize": "50kb"
    }
  ],
  "size-limit": [
    {
      "path": "dist/index.js",
      "limit": "100 KB"
    }
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-push": "npm run test && npm run security:audit"
    }
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ],
    "*.{yml,yaml}": [
      "prettier --write",
      "yaml-lint",
      "git add"
    ],
    "*.{json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "release": {
    "branches": [
      "main",
      "master"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git",
      "@semantic-release/github"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ],
  "files": [
    "dist",
    "lib",
    "src",
    "*.yml",
    "*.yaml",
    "*.md",
    "LICENSE"
  ],
  "directories": {
    "lib": "lib",
    "test": "test",
    "doc": "docs"
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/straydogsyndications"
  },
  "private": false,
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
