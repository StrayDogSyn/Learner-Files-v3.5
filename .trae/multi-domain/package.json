{
  "name": "trae-multi-domain-portfolio",
  "version": "1.0.0",
  "description": "Trae automation system for managing multiple portfolio domains with deployment, monitoring, and optimization",
  "main": "index.js",
  "scripts": {
    "setup": "powershell -ExecutionPolicy Bypass -File setup.ps1",
    "setup:dry-run": "powershell -ExecutionPolicy Bypass -File setup.ps1 -DryRun",
    "setup:skip-validation": "powershell -ExecutionPolicy Bypass -File setup.ps1 -SkipValidation",
    "setup:verbose": "powershell -ExecutionPolicy Bypass -File setup.ps1 -Verbose",
    "init": "trae init --config trae-multi-domain.yml",
    "deploy": "trae deploy --workflow full_deployment",
    "deploy:emergency": "trae deploy --workflow emergency_rollback",
    "deploy:maintenance": "trae deploy --workflow scheduled_maintenance",
    "deploy:primary": "trae deploy --target primary --config deployment-automation.yml",
    "deploy:business": "trae deploy --target business --config deployment-automation.yml",
    "deploy:org": "trae deploy --target organization --config deployment-automation.yml",
    "deploy:github": "trae deploy --target github_pages --config deployment-automation.yml",
    "sync": "trae sync --config content-sync.yml",
    "sync:real-time": "trae sync --mode real-time --config content-sync.yml",
    "sync:scheduled": "trae sync --mode scheduled --config content-sync.yml",
    "sync:manual": "trae sync --mode manual --config content-sync.yml",
    "validate": "trae validate --config link-validation.yml",
    "validate:links": "trae validate links --config link-validation.yml",
    "validate:cross-domain": "trae validate cross-domain --config link-validation.yml",
    "validate:pre-deployment": "trae validate pre-deployment --config link-validation.yml",
    "monitor": "trae monitor --config monitoring-config.yml",
    "monitor:start": "trae monitor start --config monitoring-config.yml",
    "monitor:stop": "trae monitor stop --config monitoring-config.yml",
    "monitor:status": "trae monitor status --config monitoring-config.yml",
    "health": "trae health check --comprehensive",
    "health:quick": "trae health check --quick",
    "health:domains": "trae health check --domains",
    "health:ssl": "trae health check --ssl",
    "ssl": "trae ssl --config ssl-management.yml",
    "ssl:renew": "trae ssl renew --config ssl-management.yml",
    "ssl:check": "trae ssl check --config ssl-management.yml",
    "ssl:provision": "trae ssl provision --config ssl-management.yml",
    "analytics": "trae analytics --config analytics-reporting.yml",
    "analytics:report": "trae analytics report --config analytics-reporting.yml",
    "analytics:dashboard": "trae analytics dashboard --config analytics-reporting.yml",
    "dashboard": "trae dashboard open --type realtime",
    "dashboard:performance": "trae dashboard open --type performance",
    "dashboard:security": "trae dashboard open --type security",
    "dashboard:analytics": "trae dashboard open --type analytics",
    "logs": "trae logs --follow",
    "logs:errors": "trae logs --level error",
    "logs:deployment": "trae logs --category deployment",
    "logs:monitoring": "trae logs --category monitoring",
    "rollback": "trae rollback --interactive",
    "rollback:last": "trae rollback --to-last-stable",
    "rollback:version": "trae rollback --to-version",
    "backup": "trae backup create --all-domains",
    "backup:restore": "trae backup restore --interactive",
    "test": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "test:unit": "jest --config jest.config.js --testPathPattern=unit",
    "test:integration": "jest --config jest.config.js --testPathPattern=integration",
    "test:e2e": "playwright test --config playwright.config.js",
    "test:performance": "lighthouse-ci autorun",
    "test:security": "snyk test",
    "test:accessibility": "axe-cli",
    "lint": "eslint . --ext .js,.ts,.yml,.yaml",
    "lint:fix": "eslint . --ext .js,.ts,.yml,.yaml --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "validate:config": "trae config validate --all",
    "validate:env": "trae env validate",
    "docs": "trae docs generate",
    "docs:serve": "trae docs serve --port 3000",
    "clean": "trae clean --cache --logs --temp",
    "reset": "trae reset --confirm",
    "update": "npm update && trae update",
    "security:audit": "npm audit && snyk test",
    "dev": "trae dev --watch --hot-reload",
    "dev:primary": "trae dev --target primary --watch",
    "dev:business": "trae dev --target business --watch",
    "dev:org": "trae dev --target organization --watch",
    "dev:github": "trae dev --target github_pages --watch",
    "build": "trae build --all-targets",
    "build:primary": "trae build --target primary",
    "build:business": "trae build --target business",
    "build:org": "trae build --target organization",
    "build:github": "trae build --target github_pages",
    "preview": "trae preview --all-domains",
    "preview:primary": "trae preview --domain straydog-syndications-llc.com",
    "preview:business": "trae preview --domain straydogsyndicationsllc.biz",
    "preview:org": "trae preview --domain straydog-secondstory.org",
    "preview:github": "trae preview --domain straydogsyndications.github.io",
    "status": "trae status --detailed",
    "status:quick": "trae status --quick",
    "status:domains": "trae status --domains",
    "status:deployments": "trae status --deployments",
    "status:monitoring": "trae status --monitoring",
    "config:show": "trae config show --all",
    "config:validate": "trae config validate --strict",
    "config:update": "trae config update --interactive",
    "env:show": "trae env show --masked",
    "env:validate": "trae env validate --strict",
    "env:template": "trae env template --output .env.example",
    "maintenance:start": "trae maintenance start --all-domains",
    "maintenance:end": "trae maintenance end --all-domains",
    "maintenance:status": "trae maintenance status",
    "cache:clear": "trae cache clear --all",
    "cache:warm": "trae cache warm --all-domains",
    "cache:status": "trae cache status",
    "notifications:test": "trae notifications test --all-channels",
    "notifications:config": "trae notifications config --interactive",
    "performance:audit": "trae performance audit --all-domains",
    "performance:optimize": "trae performance optimize --all-domains",
    "performance:report": "trae performance report --detailed",
    "seo:audit": "trae seo audit --all-domains",
    "seo:optimize": "trae seo optimize --all-domains",
    "seo:report": "trae seo report --detailed",
    "security:scan": "trae security scan --all-domains",
    "security:report": "trae security report --detailed",
    "security:fix": "trae security fix --interactive",
    "compliance:check": "trae compliance check --all-standards",
    "compliance:report": "trae compliance report --detailed",
    "integration:test": "trae integration test --all-services",
    "integration:status": "trae integration status",
    "workflow:list": "trae workflow list",
    "workflow:run": "trae workflow run --interactive",
    "workflow:status": "trae workflow status --active",
    "alerts:list": "trae alerts list --active",
    "alerts:acknowledge": "trae alerts acknowledge --interactive",
    "alerts:history": "trae alerts history --last-30-days",
    "reports:generate": "trae reports generate --all-types",
    "reports:schedule": "trae reports schedule --interactive",
    "reports:export": "trae reports export --format pdf",
    "debug": "trae debug --verbose --include-logs",
    "debug:deployment": "trae debug deployment --last-failed",
    "debug:monitoring": "trae debug monitoring --check-all",
    "debug:performance": "trae debug performance --analyze-bottlenecks",
    "troubleshoot": "trae troubleshoot --interactive",
    "troubleshoot:network": "trae troubleshoot network --all-domains",
    "troubleshoot:ssl": "trae troubleshoot ssl --all-certificates",
    "troubleshoot:deployment": "trae troubleshoot deployment --last-failed",
    "help": "trae --help",
    "version": "trae --version",
    "info": "trae info --system --config --status"
  },
  "keywords": [
    "trae",
    "automation",
    "multi-domain",
    "portfolio",
    "deployment",
    "monitoring",
    "seo",
    "performance",
