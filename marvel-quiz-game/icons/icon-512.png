<svg xmlns="http://www.w3.org/2000/svg" width="512" height="512" viewBox="0 0 512 512">
  <defs>
    <linearGradient id="marvelGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#ff6b6b;stop-opacity:1" />
      <stop offset="25%" style="stop-color:#4ecdc4;stop-opacity:1" />
      <stop offset="50%" style="stop-color:#45b7d1;stop-opacity:1" />
      <stop offset="75%" style="stop-color:#96ceb4;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#feca57;stop-opacity:1" />
    </linearGradient>
    <filter id="glow">
      <feGaussianBlur stdDeviation="8" result="coloredBlur"/>
      <feMerge> 
        <feMergeNode in="coloredBlur"/>
        <feMergeNode in="SourceGraphic"/>
      </feMerge>
    </filter>
    <filter id="innerGlow">
      <feGaussianBlur stdDeviation="4" result="coloredBlur"/>
      <feMerge> 
        <feMergeNode in="coloredBlur"/>
        <feMergeNode in="SourceGraphic"/>
      </feMerge>
    </filter>
  </defs>
  
  <!-- Background -->
  <rect width="512" height="512" rx="64" fill="#0a0a23"/>
  
  <!-- Glassmorphic layers -->
  <rect x="32" y="32" width="448" height="448" rx="56" fill="url(#marvelGradient)" opacity="0.1"/>
  <rect x="32" y="32" width="448" height="448" rx="56" fill="none" stroke="url(#marvelGradient)" stroke-width="4" opacity="0.3"/>
  <rect x="64" y="64" width="384" height="384" rx="48" fill="rgba(255,255,255,0.05)"/>
  
  <!-- Marvel Logo Style -->
  <g transform="translate(256,256)" filter="url(#glow)">
    <!-- Large M -->
    <path d="M-160,-80 L-160,80 L-130,80 L-130,0 L-100,50 L-70,0 L-70,80 L-40,80 L-40,-80 L-85,-80 L-100,-30 L-115,-80 Z" fill="#ff6b6b" stroke="#ff6b6b" stroke-width="2"/>
    
    <!-- Quiz Symbol -->
    <circle cx="0" cy="0" r="70" fill="none" stroke="#4ecdc4" stroke-width="8" filter="url(#innerGlow)"/>
    <circle cx="0" cy="0" r="50" fill="rgba(69,183,209,0.2)"/>
    <text x="0" y="20" text-anchor="middle" font-family="Arial, sans-serif" font-size="64" font-weight="bold" fill="#45b7d1" filter="url(#innerGlow)">?</text>
    
    <!-- Decorative cosmic elements -->
    <circle cx="90" cy="-40" r="8" fill="#96ceb4" opacity="0.8" filter="url(#innerGlow)"/>
    <circle cx="100" cy="30" r="6" fill="#feca57" opacity="0.8" filter="url(#innerGlow)"/>
    <circle cx="-90" cy="40" r="7" fill="#ff6b6b" opacity="0.8" filter="url(#innerGlow)"/>
    <circle cx="-80" cy="-50" r="5" fill="#4ecdc4" opacity="0.8" filter="url(#innerGlow)"/>
    
    <!-- Additional cosmic particles -->
    <circle cx="120" cy="-10" r="3" fill="#96ceb4" opacity="0.6"/>
    <circle cx="-110" cy="10" r="4" fill="#feca57" opacity="0.6"/>
    <circle cx="70" cy="80" r="3" fill="#ff6b6b" opacity="0.6"/>
    <circle cx="-70" cy="-80" r="3" fill="#45b7d1" opacity="0.6"/>
  </g>
  
  <!-- Bottom text -->
  <text x="256" y="450" text-anchor="middle" font-family="Arial, sans-serif" font-size="32" font-weight="bold" fill="#4ecdc4" filter="url(#innerGlow)">ULTIMATE MARVEL QUIZ</text>
  
  <!-- Subtitle -->
  <text x="256" y="480" text-anchor="middle" font-family="Arial, sans-serif" font-size="16" fill="#96ceb4" opacity="0.8">10x Development Showcase</text>
</svg>